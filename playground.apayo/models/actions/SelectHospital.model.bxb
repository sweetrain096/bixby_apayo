action (SelectHospital) {
  description (유저가 병원을 선택하도록 한다)
  type(Constructor)
  collect {
    input (hospital) {
      type (NearHospitalList)
      min (Optional) max (One)
      default-init{
        intent{
          goal: GetHospitalList
        }
      }
    }
    input (selectNum){
      type(entity.Ordinal)
      min (Optional) max (One)
    }
  }
  
  
  output (HospitalSummaryInfo)
}
